<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.demo.study">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
            android:name=".MyApp"
            android:allowBackup="true"
            android:icon="@drawable/logo"
            android:label="@string/app_name"
            android:roundIcon="@drawable/logo"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.ac.main.MainActivity"
                  android:theme="@style/mainTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.ac.home.HomeActivity"/>
        <activity android:name=".ui.ac.course.coursedetail.CourseDetailActivity"/>
        <activity android:name=".ui.ac.mine.login.LoginActivity"/>
        <activity android:name=".ui.ac.mine.userinfo.UserInfoActivity"/>
        <activity android:name=".ui.ac.mine.userinfo.ChooseImgActivity"/>
        <activity android:name=".ui.ac.course.buycourse.BuyCourseActivity"/>
        <activity android:name=".ui.ac.course.playvideo.PlayVideoActivity"
                  android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name=".ui.ac.mine.wallet.wallet.WalletActivity"/>
        <activity android:name=".ui.ac.mine.wallet.walletdetail.WalletDetailActivity"/>
    </application>

</manifest>